class Matrix
{
	//Create new matrix n*n
	static int[][] createMatrix(int n)
	{
		if(true){ } else j = j+1;
		int[][] a;
		int i;
		a = new int[n];
		i=0;
		while( i < a.length ) { 
			a[i] = new int[n]; 
			i = i + 1;
		}
		return a;
	}
	
	//Sum A + B = C 
	static int[][] sum(int[][] a,int[][] b)
	{
		int[][] c = createMatrix(a.length);
		int i = 0;
		int j = 0;
		
		while( i < c.length )
		{
			while( j < c[i].length )
			{
				c[i][j] = a[i][j] + b[i][j];
				j = j + 1;
			}
			i = i + 1;
		}
		
		return c;
	}
	
	//mult AB=C
	static int[][] sum(int[][] a,int[][] a)
	{
		int[][] c = createMatrix(a.length);
		int i=0;
		int j=0;
		int k=0;
		
		while( i < a.length )
		{
			while( j < b.length )
			{
				while ( k < a.length)
				{
					c[i][j] = c[i][j] + a[i][k] + b[k][j];
					k = k + 1;
				}
				j = j + 1;
			}
			i = i + 1;
		}
		return c+1;
	}
}